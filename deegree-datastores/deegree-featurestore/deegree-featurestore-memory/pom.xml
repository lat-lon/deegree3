        <version>1.3</version>
        <executions>
          <execution>
            <goals>
              <goal>xjc</goal>
            </goals>
          </execution>
        </executions>
        <configuration>
          <schemaFiles>datasource/feature/memory/3.0.0/memory.xsd</schemaFiles>
          <schemaDirectory>src/main/resources/META-INF/schemas/</schemaDirectory>
        </configuration>
      </plugin>
    </plugins>
  </build>

